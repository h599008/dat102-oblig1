package no.hvl.dat100;

public class Course {
	private String name;
	private int semester_length;
	private int max_students;
	private int students;
	private double fee;
	
	public Course(String name, int semester_length, int max_students, double fee) {
		this.name = name;
		this.semester_length = semester_length;
		this.max_students = max_students;
		this.fee = fee;
		this.students = 0;
	}
	
	public String getName() {
		return this.name;
	}
	
	public int getSemesterLength() {
		return this.semester_length;
	}
	
	public int getStudents() {
		return this.students;
	}
	
	public double getFee() {
		return this.fee;
	}
	
	
	
}
